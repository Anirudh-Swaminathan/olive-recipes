{
    "input_model": {
        "type": "PyTorchModel",
        "model_path": "google-bert/bert-base-multilingual-cased",
        "io_config": {
            "input_names": [
                "input_ids",
                "attention_mask",
                "token_type_ids"
            ],
            "input_shapes": [
                [
                    1,
                    128
                ],
                [
                    1,
                    1,
                    128,
                    128
                ],
                [
                    1,
                    128
                ]
            ],
            "input_types": [
                "int32",
                "float32",
                "int32"
            ],
            "output_names": [
                "logits"
            ]
        },
        "model_loader": "load_bert_nsp_model",
        "model_script": "google_bert.py"
    },
    "systems": {
        "host_system": {
            "type": "LocalSystem",
            "accelerators": [
                {
                    "execution_providers": [
                        "CPUExecutionProvider"
                    ]
                }
            ]
        },
        "target_system": {
            "type": "LocalSystem",
            "accelerators": [
                {
                    "execution_providers": [
                        "QNNExecutionProvider"
                    ]
                }
            ]
        }
    },
    "data_configs": [
        {
            "name": "calibration_data",
            "type": "HuggingfaceContainer",
            "user_script": "google_bert.py",
            "load_dataset_config": {
                "data_name": "glue",
                "subset": "mrpc",
                "split": "train"
            },
            "pre_process_data_config": {
                "type": "tokenize_dataset",
                "input_cols": [
                    "sentence1",
                    "sentence2"
                ],
                "label_col": "label",
                "seq_length": 128,
                "model_name": "google-bert/bert-base-multilingual-cased",
                "max_samples": 10
            },
            "dataloader_config": {
                "batch_size": 1
            }
        },
        {
            "name": "wiki_data",
            "type": "HuggingfaceContainer",
            "load_dataset_config": {
                "type": "dataset_to_nsp_dataset",
                "data_name": "wikitext",
                "subset": "wikitext-2-raw-v1",
                "split": "train",
                "input_cols": [
                    "sentence1",
                    "sentence2"
                ],
                "label_col": "label"
            },
            "pre_process_data_config": {
                "type": "tokenize_dataset",
                "model_name": "google-bert/bert-base-multilingual-cased",
                "input_cols": [
                    "sentence1",
                    "sentence2"
                ],
                "label_col": "label",
                "seq_length": 128,
                "max_samples": 10
            },
            "post_process_data_config": {
                "type": "bert_scl_post_process"
            },
            "dataloader_config": {
                "batch_size": 1
            },
            "user_script": "google_bert.py"
        }
    ],
    "evaluators": {
        "glue_evaluator": {
            "metrics": [
                {
                    "name": "accuracy",
                    "type": "accuracy",
                    "backend": "huggingface_metrics",
                    "data_config": "wiki_data",
                    "sub_types": [
                        {
                            "name": "accuracy",
                            "priority": 1
                        },
                        {
                            "name": "f1"
                        }
                    ]
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [
                        {
                            "name": "avg",
                            "priority": 2,
                            "metric_config": {
                                "warmup_num": 20,
                                "repeat_test_num": 100
                            }
                        }
                    ]
                }
            ]
        }
    },
    "passes": {
        "conversion": {
            "type": "OnnxConversion",
            "target_opset": 20,
            "save_as_external_data": true
        },
        "surgery": {
            "type": "GraphSurgeries",
            "surgeries": [
                {
                    "surgeon": "ReplaceAttentionMaskValue"
                },
                {
                    "surgeon": "MatMulAddToGemm"
                }
            ]
        },
        "quantization": {
            "type": "OnnxStaticQuantization",
            "data_config": "calibration_data",
            "quant_preprocess": true,
            "calibrate_method": "MinMax",
            "activation_type": "uint16",
            "precision": "uint8",
            "save_as_external_data": true
        }
    },
    "host": "host_system",
    "target": "target_system",
    "cache_dir": "cache",
    "evaluator": "glue_evaluator",
    "evaluate_input_model": false,
    "output_dir": "model/google_bert_base_multilingual_cased_qdq"
}
